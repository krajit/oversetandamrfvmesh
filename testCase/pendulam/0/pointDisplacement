FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0.045";
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    sides
    {
        type            uniformFixedValue;
        value           uniform (0 0 0);
        uniformValue    constant (0 0 0);
    }
    inlet
    {
        type            uniformFixedValue;
        value           uniform (0 0 0);
        uniformValue    constant (0 0 0);
    }
    outlet
    {
        type            uniformFixedValue;
        value           uniform (0 0 0);
        uniformValue    constant (0 0 0);
    }
    ballOut
    {
        type            zeroGradient;
        patchType       overset;
    }
    ballIn
    {
        type            calculated; //uniformFixedValue;
        // value           uniform (0 0 0);
        // uniformValue    constant (0 0 0);
    }
    frontAndBack
    {
        type            empty;
    }
}


