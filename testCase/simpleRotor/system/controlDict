FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs            ("liboverset.so" "liboversetAndAMR.so");

application     overPimpleDyMFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1e-2;

deltaT          1e-5;;

writeControl    timeStep;
writeInterval   10;

//writeControl    adjustableRunTime;
//writeInterval   0.00025; //0.01;

purgeWrite      0;

writeFormat     binary; //ascii;

writePrecision  10;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  no; //yes;

maxCo           1;


functions
{

	refinementFieldFunction1
    {
        type        refinementFieldFunction;
        libs        ("librefinementfieldFunctionObjects.so");
		writeControl  writeTime;   
		xBuffer      0.0005;
		yBuffer      0.0005;
		zBuffer      0.001;
     
    }

}

